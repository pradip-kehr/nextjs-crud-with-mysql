generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

model Post {
    id         Int      @id @default(autoincrement())
    user_id    Int
    title      String
    body       String
    created_at DateTime @default(now())
    updated_at DateTime @updatedAt
}
